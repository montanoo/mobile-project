    "@react-native/typescript-config": "0.74.83",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
